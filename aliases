# non private aliases - simply add to bashrc
# with 'source .aliases'

# Rails 2
alias ss='ruby script/server --debugger'
alias sc='ruby script/console'

# Git
alias gs='git status'
alias ga='git add'
alias gc='git commit'
alias gb='git branch'
alias gd='git diff'
alias gco='git checkout'
alias gl='git log'
